{
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"eject": "expo eject",
		"test": "jest",
		"build:docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
		"destroy:docs": "rm -rd ./docs",
		"watch:doc-src": "watch \"npm run build:docs\" ./src",
		"format": "prettier --write --config ./.prettierrc src/**/*{.tsx,.json,.js}"
	},
	"dependencies": {
		"@expo-google-fonts/montserrat": "0.1.0",
		"@expo/metro-config": "0.1.59",
		"@expo/ngrok": "2.4.3",
		"@types/jest": "26.0.21",
		"axios": "0.21.1",
		"better-docs": "2.3.2",
		"expo": "40.0.1",
		"expo-app-loading": "1.0.1",
		"expo-cli": "4.3.1",
		"expo-font": "9.0.0",
		"expo-localization": "~9.1.0",
		"expo-status-bar": "~1.0.3",
		"i18n-js": "3.8.0",
		"install": "0.13.0",
		"jsdoc": "3.6.6",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
		"react-native-global-props": "1.1.5",
		"react-native-web": "~0.13.12",
		"tslib": "2.1.0"
	},
	"devDependencies": {
		"@babel/core": "~7.9.0",
		"@babel/plugin-transform-typescript": "7.13.0",
		"@babel/preset-typescript": "7.13.0",
		"@types/i18n-js": "3.8.0",
		"@types/mocha": "8.2.2",
		"@types/react": "~16.9.35",
		"@types/react-dom": "~16.9.8",
		"@types/react-native": "~0.63.2",
		"@types/react-native-global-props": "1.1.0",
		"@types/react-test-renderer": "17.0.1",
		"@typescript-eslint/eslint-plugin": "4.19.0",
		"@typescript-eslint/parser": "4.19.0",
		"babel-plugin-module-resolver": "4.1.0",
		"eslint": "7.21.0",
		"eslint-config-universe": "7.0.1",
		"eslint-plugin-react-native": "3.10.0",
		"jest-expo": "40.0.2",
		"jsdoc-babel": "0.5.0",
		"livereload": "0.9.2",
		"prettier": "2.2.1",
		"react-test-renderer": "17.0.1",
		"typescript": "~4.0.0"
	},
	"jest": {
		"preset": "jest-expo",
		"transformIgnorePatterns": [
			"node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
		]
	},
	"eslintConfig": {
		"extends": "universe/native"
	},
	"private": true
}
