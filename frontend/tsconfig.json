{
    "compilerOptions": {
        "noEmit": true,
        "lib": [ "dom", "esnext" ],
        "jsx": "react-native",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "strictBindCallApply": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "baseUrl": "./src"
    }
}
