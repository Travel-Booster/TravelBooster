{
    "compilerOptions": {
        "noEmit": true,
        "lib": [ "dom", "esnext" ],
        "jsx": "react-native",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "strictBindCallApply": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "baseUrl": "./src",      
        "target": "ESNext",
        "module":"CommonJS",
        "sourceMap": true,
        "esModuleInterop": true
    },
    "typedocOptions": {
        "entryPoints": ["./src"],
        "out": "docs",
        "name": "TravelBooster TypeDoc",
        "theme": "minimal"
    }
}
